<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Spinner?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <AnchorPane>
         <children>
            <BorderPane prefHeight="397.0" prefWidth="404.0">
               <bottom>
                  <HBox prefHeight="33.0" prefWidth="404.0" BorderPane.alignment="CENTER">
                     <children>
                        <Spinner fx:id="p" />
                        <Spinner fx:id="i" />
                        <Spinner fx:id="d" />
                     </children>
                  </HBox>
               </bottom>
               <center>
                  <ImageView fitHeight="269.0" fitWidth="283.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER" />
               </center>
               <left>
                  <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                     <children>
                        <Button mnemonicParsing="false" text="Button" />
                        <Button mnemonicParsing="false" text="Button" />
                        <Button mnemonicParsing="false" text="Button" />
                        <Button mnemonicParsing="false" text="Button" />
                        <Button fx:id="send" mnemonicParsing="false" onAction="#sendPackage" text="Send" />
                     </children>
                  </VBox>
               </left>
               <top>
                  <TextField fx:id="ip" BorderPane.alignment="CENTER" />
               </top>
            </BorderPane>
         </children>
      </AnchorPane>
   </children>
</GridPane>
